{
  "name": "nucleus-professional",
  "version": "3.1.0-professional",
  "description": "Enterprise AI Platform with Advanced Orchestration and Professional Management",
  "main": "dist/nucleus-engine.js",
  "types": "dist/nucleus-engine.d.ts",
  "type": "module",
  "license": "Commercial",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node dist/nucleus-engine.js",
    "dev": "tsx src/nucleus-engine.ts",
    "build": "tsx build/build-system.ts",
    "build:dev": "tsx build/build-system.ts --dev",
    "build:prod": "tsx build/build-system.ts --mode=production",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:watch": "jest --watch",
    "test:performance": "node test/performance/load-test.js",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "security:audit": "npm audit --audit-level=moderate",
    "security:scan": "npx detect-secrets scan --all-files",
    "docker:build": "docker build -t nucleus:latest .",
    "docker:run": "docker run -p 3000:3000 nucleus:latest",
    "docker:compose": "docker-compose up -d",
    "k8s:deploy": "kubectl apply -f deployments/",
    "k8s:status": "kubectl get pods -n nucleus-production",
    "deploy:dev": "tsx scripts/production-deploy.ts --staging",
    "deploy:staging": "tsx scripts/production-deploy.ts --staging",
    "deploy:production": "tsx scripts/production-deploy.ts",
    "rollback": "kubectl rollout undo deployment/nucleus-deployment -n nucleus-production",
    "health": "curl http://localhost:3000/health",
    "metrics": "curl http://localhost:3000/metrics",
    "logs": "kubectl logs -f deployment/nucleus-deployment -n nucleus-production",
    "clean": "rm -rf dist node_modules/.cache",
    "docs:generate": "typedoc src --out docs",
    "docs:serve": "cd docs && python -m http.server 8080",
    "performance:monitor": "node scripts/performance-monitor.js",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "dev:unified": "NODE_ENV=development tsx launch-unified.ts",
    "dev:nicholas": "NODE_ENV=development tsx server/index.ts",
    "dev:ai": "NODE_ENV=development tsx nucleus/simple-test.ts",
    "start:unified": "NODE_ENV=production tsx unified-nicholas-empire.ts",
    "test:unified": "tsx launch-unified.ts",
    "test:complete": "npm run test && npm run test:unified",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "unified": "tsx launch-unified.ts",
    "nicholas": "tsx server/index.ts",
    "ai": "tsx nucleus/simple-test.ts"
  },
